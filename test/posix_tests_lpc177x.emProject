      arm_compiler_variant="gcc"
      arm_core_type="Cortex-M3"
      arm_endian="Little"
      arm_fp_abi="Soft"
      arm_fpu_type=""
      arm_linker_heap_size="8192"
      arm_linker_process_stack_size="2048"
